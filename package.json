{
	"name": "@das-buro-am-draht/directus-flow-sync",
	"description": "Import and export directus flows and operations",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook"
	],
	"type": "module",
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.mjs",
		"source": "src/index.ts",
		"host": "^9.26.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"clean": "rimraf dist",
		"deploy": "cp -r dist/* extensions/hooks/flow-sync/",
		"dev": "directus-extension build -w --no-minify",
		"format": "prettier --write \"src/**/*.ts\"",
		"link": "directus-extension link",
		"lint": "eslint . --ext .ts",
		"test": "jest",
		"export": "npx directus flow export",
		"import": "npx directus flow import"
	},
	"devDependencies": {
		"@commander-js/extra-typings": "^11.0.0",
		"@directus/extensions-sdk": "^10.1.9",
		"@directus/types": "^10.1.5",
		"@stylistic/eslint-plugin": "^1.2.0",
		"@types/jest": "^29.5.5",
		"@types/node": "^20.6.0",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"eslint": "^8.53.0",
		"jest": "^29.7.0",
		"pino": "^8.15.1",
		"rimraf": "^5.0.1",
		"ts-jest": "^29.1.1",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"dotenv": "^16.3.1",
		"zod": "^3.22.2"
	}
}
